{% if type == 'block' %}
  {% assign id = block.id %}
  {% assign content_page = block.settings.content_page %}
{% else %}
  {% assign id = section.id %}
  {% assign content_page = section.settings.content_page %}
{% endif %}

<div class="homepage_content featured_content clearfix">
  <div class="container">
    <div class="sixteen columns">
      <div class="section clearfix">
        {% if content_page != blank %}
          {% include 'page-multi-column', content: pages[content_page].content %}
        {% else %}
          {{ 'homepage.onboarding.no_content' | t }}
        {% endif %}
      </div>
    </div>
  </div>
</div>